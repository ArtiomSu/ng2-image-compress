{
  "name": "ng2-imagecompress-app",
  "version": "11.2.0-wip",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "purgemaps": "rm dist/ng2-image-compress/bundles/artiomsu-ng2-image-compress.umd.js.map && rm dist/ng2-image-compress/fesm2015/artiomsu-ng2-image-compress.js.map",
    "build-lib": "ng build ng2-image-compress --prod && npm run purgemaps",
    "npm-pack": "cd dist/ng2-image-compress && npm pack",
    "package": "npm run build-lib && npm run npm-pack",
    "publish": "npm publish $(command ls dist/ng2-image-compress/*.tgz)",
    "packagepublish": "npm run package && npm run publish"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.3",
    "@angular/cdk": "^11.2.2",
    "@angular/common": "~11.2.3",
    "@angular/compiler": "~11.2.3",
    "@angular/core": "~11.2.3",
    "@angular/forms": "~11.2.3",
    "@angular/material": "^11.2.2",
    "@angular/platform-browser": "~11.2.3",
    "@angular/platform-browser-dynamic": "~11.2.3",
    "@angular/router": "~11.2.3",
    "core-js": "^2.5.4",
    "rxjs": "^6.6.6",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.2",
    "@angular/cli": "~11.2.2",
    "@angular/compiler-cli": "~11.2.3",
    "@angular/language-service": "~11.2.3",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~6.0.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^11.2.4",
    "ts-node": "~7.0.0",
    "tslib": "^1.9.0",
    "tslint": "^6.1.3",
    "typescript": "~4.1.5"
  }
}
